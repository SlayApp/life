{
  "name": "LifeTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios",
    "lint": "yarn lint:js && yarn lint:ts && yarn prettier:check",
    "lint:js": "eslint '**/*.{ts,tsx}' --ignore-pattern='backend/','react-native-godot/'",
    "lint:ts": "tsc -p ./tsconfig.json --noEmit",
    "start": "npx react-native start",
    "test": "jest",
    "postinstall": "npx patch-package",
    "generate:apis": "openapi-generator-cli generate -i backend/openapi.yaml -g typescript-axios -o api-client --skip-validate-spec && yarn upgrade api-client@portal:./api-client",
    "generate:asyncapi": "openapi-typescript backend/asyncapi.yaml -o ./src/types/asyncapi.d.ts --root-types --root-types-no-schema-prefix --export-type --enum --make-paths-enum && prettier --write ./src/types/asyncapi.d.ts"
  },
  "codegenConfig": {
    "name": "NativeTextInputHelpers",
    "type": "modules",
    "jsSrcsDir": "specs",
    "ios": {
      "modulesProvider": {
        "NativeTextInputHelpers": "RCTNativeTextInputHelpers"
      }
    }
  },
  "dependencies": {
    "@legendapp/list": "^1.0.11",
    "@react-navigation/elements": "^2.4.2",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@shopify/flash-list": "^1.8.0",
    "@shopify/react-native-skia": "^2.0.1",
    "@tanstack/query-sync-storage-persister": "^5.77.0",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-query-persist-client": "^5.77.0",
    "api-client": "^1.1.3",
    "awesome-phonenumber": "^7.5.0",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "expo": "~53.0.0",
    "expo-blur": "~14.1.4",
    "expo-crypto": "~14.1.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-localization": "~16.1.5",
    "expo-symbols": "~0.4.4",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-bootsplash": "^6.3.8",
    "react-native-date-picker": "^5.0.12",
    "react-native-edge-to-edge": "^1.6.0",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-keyboard-controller": "^1.17.1",
    "react-native-mmkv": "^3.2.0",
    "react-native-nitro-modules": "^0.25.2",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-unistyles": "^3.0.0-rc.3",
    "react-native-url-polyfill": "^2.0.0",
    "socket.io-client": "^4.8.1",
    "unstated-next": "^1.1.0",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@eslint/compat": "^1.2.9",
    "@openapitools/openapi-generator-cli": "2.15.3",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.6.3",
    "openapi-typescript": "^6.7.5",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.32.0"
  },
  "engines": {
    "node": ">=18"
  }
}
